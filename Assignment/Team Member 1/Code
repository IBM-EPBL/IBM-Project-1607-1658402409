#include<Servo.h>
Servo myservo;
int led=6;
int pin=2;
void setup()
{
  Serial.begin(9600);
  pinMode(pin,INPUT);
  pinMode(led,OUTPUT);
  myservo.attach(9);
}
void loop()
{
  int val=digitalRead(pin);
  Serial.println(val);
  if(val==HIGH){
    digitalWrite(led,HIGH);
    myservo.write(70);
  }
  else{
    digitalWrite(led,LOW);
    myservo.write(10);
  }
}
